{
    "name": "danek/feed",
    "description": "Fork feed-io for (PHP >= 7.1 && PHP < 8) - PHP library built to consume and serve JSONFeed / RSS / Atom feeds",
    "keywords": ["rss", "atom","jsonfeed", "feed", "news", "CLI", "client"],
    "homepage": "https://feed-io.net",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Debril",
            "email": "alex.debril@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Alexandre Debril",
            "homepage": "https://jdanek.eu",
            "role": "Fork developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "~6.2|~7.0",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Danek\\FeedIo\\": "src"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.1.99"
        }
    }
}
